{
    "vscode_custom_css.imports": [

        "file:///path/to/.config/vscode.css",
    ],
    "vscode_custom_css.statusbar": false,
    "debug.allowBreakpointsEverywhere": true,
    "workbench.startupEditor": "none",
    "window.nativeTabs": false,
    "workbench.activityBar.location": "top",
    "workbench.sideBar.location": "left",
    "workbench.editor.showIcons": false,
    "editor.minimap.enabled": false,
    "debug.showBreakpointsInOverviewRuler": true,
    "editor.scrollbar.verticalScrollbarSize": 4,
    "editor.scrollbar.horizontalScrollbarSize": 4,
    "editor.renderWhitespace": "none",
    "security.workspace.trust.untrustedFiles": "open",
    "editor.mouseWheelZoom": true,
    "editor.folding": false,
    "window.title": "${activeEditorLong}",
    "editor.showFoldingControls": "never",
    "files.autoGuessEncoding": true,
    "[python]": {
        "editor.formatOnType": true
    },
    "update.showReleaseNotes": false,
    "telemetry.telemetryLevel": "off",
    "extensions.autoUpdate": false,
    "window.zoomLevel": 2,
    "editor.selectionHighlight": false,
    "editor.cursorBlinking": "solid",
    "editor.cursorSmoothCaretAnimation": "off",
    "editor.cursorStyle": "block",
    "editor.guides.bracketPairs": true,
    "editor.rulers": [80],
    "editor.renderLineHighlight": "all",
    "editor.wordSegmenterLocales": "ja",
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "search.showLineNumbers": true,
    "workbench.editor.wrapTabs": true,
    "git.enableSmartCommit": true,
    "git.autofetch": true,
    "git.confirmSync": false,
    "terminal.explorerKind": "external",
    "terminal.external.linuxExec": "",
    "terminal.external.osxExec": "/Applications/iTerm.app",
    "terminal.sourceControlRepositoriesKind": "external",
    "diffEditor.ignoreTrimWhitespace": false,
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/**": true,
        "**/tmp/**": true
    },
    "search.exclude": {
        "**/node_modules": true,
        "**/out": true
    },
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/.DS_Store": true
    },
    "workbench.editor.limit.enabled": true,
    "workbench.editor.limit.value": 10,
    "workbench.colorCustomizations": {
        "window.titleBarStyle": "custom",
        "window.menuBarVisibility": "toggle",
        "statusBar.background": "#f701ff",
        "statusBar.noFolderBackground": "#f701ff",
        "statusBar.debuggingBackground": "#f701ff",
        "statusBar.foreground": "#000000",
        "statusBar.noFolderForeground": "#000000",
        "statusBar.debuggingForeground": "#000000",
        "panel.border": "#00000000",
        "editorGroup.border": "#00000000",
        "editor.border": "#00000000",
        "activityBar.border": "#00000000",
        "sideBar.border": "#00000000",
        "statusBar.border": "#00000000",
        "editorPane.border": "#00000000",
        "button.border": "#00000000",
        "input.border": "#00000000",
        "list.activeSelectionBorder": "#00000000",
        "editor.selectionBackground": "#4a4b4ab7",
        "editorGroup.focusedBorder": "#00000000",
        "editorGroup.emptyBorder": "#00000000",
        "list.focusBorder": "#00000000",
        "tree.focusBorder": "#00000000",
        "editorGutter.border": "#00000000",
        "tab.activeBorder": "#00000000",
        "tab.inactiveBorder": "#00000000",
        "tab.border": "#00000000",
        "window.titleBar.border": "#00000000",
        "titleBar.activeBorder": "#00000000",
        "titleBar.inactiveBorder": "#00000000",
        "titleBar.border": "#00000000",
        "window.activeBorder": "#00000000",
        "window.inactiveBorder": "#00000000",
        "editorGroupHeader.border": "#00000000",
        "editorGroupHeader.tabsBorder": "#00000000",
        "menu.border": "#00000000",
        "sideBarSectionHeader.border": "#00000000",
        "sideBarActivityBarTop.border": "#00000000",
        "sideBySideEditor.verticalBorder": "#00000000",
        "sideBySideEditor.horizontalBorder": "#00000000",
        "scrollbarSlider.background": "#4443439b",
        "scrollbarSlider.activeBackground": "#4443439b",
        "scrollbarSlider.hoverBackground": "#4443439b",
        "progressBar.background": "#000000",
        "editor.hoverHighlightBackground": "#1c1c1c9b",
        "editorHoverWidget.background": "#0d0d0d",
        "editorIndentGuide.background1": "#00000000",
        "tree.indentGuidesStroke": "#00000000",
        "activityBar.foreground": "#00000000",
        "activityBar.background": "#00000000",
        "editorSuggestWidget.background": "#0a0a0a",
        "editorSuggestWidget.foreground": "#d5c9c9",
        "editorSuggestWidget.border": "#00000000",
        "editorSuggestWidget.selectedBackground": "#171616ea",
        "editorHoverWidget.foreground": "#d5c9c9",
        "editorHoverWidget.border": "#00000000",
        "inputBox.border": "#00000000",
        "inputBox.activeBorder": "#00000000",
        "findWidget.border": "#00000000",
        "editorWidget.border": "#00000000",
        "editor.findMatchBorder": "#00000000",
        "editor.findMatchHighlightBorder": "#00000000",
        "editor.findRangeHighlightBorder": "#00000000",
        "quickInput.background": "#000000",
        "quickInputList.focusBackground": "#000000",
        "quickInputTitle.background": "#00000000",
        "findmatch.hoverBackground": "#4a4b4ab7",
        "editor.findMatchBackground": "#f0fc00b8",
        "editor.findMatchHighlightBackground": "#58585876",
        "editor.findRangeHighlightBackground": "#58585876",
        "editorWidget.background": "#1E1E1E",
        "editorWidget.foreground": "#ffffff",
        "input.background": "#1E1E1E",
        "input.foreground": "#ffffff",
        "diffEditor.insertedTextBackground": "#5fc15f6e",
        "diffEditor.removedTextBackground": "#ff99993d"
    },
    "update.mode": "none",
    "hexeditor.columnWidth": 8,
    "hexeditor.showDecodedText": true,
    "hexeditor.defaultEndianness": "little",
    "hexeditor.inspectorType": "aside",
    "editor.copyWithSyntaxHighlighting": false,
    "window.newWindowDimensions": "inherit",
    "editor.snippetSuggestions": "top",
    "window.nativeFullScreen": true,
    "files.trimFinalNewlines": true,
    "editor.hover.delay": 1000,
    "editor.hover.enabled": true,
    "workbench.tips.enabled": false,
    "git.decorations.enabled": false,
    "editor.lightbulb.enabled": "off",
    "editor.occurrencesHighlight": "off",
    "problems.decorations.enabled": false,
    "editor.renderControlCharacters": false,
    "editor.hideCursorInOverviewRuler": true,
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "editor.gotoLocation.multipleReferences": "goto",
    "editor.gotoLocation.multipleDefinitions": "goto",
    "editor.gotoLocation.multipleDeclarations": "goto",
    "editor.gotoLocation.multipleImplementations": "goto",
    "editor.gotoLocation.multipleTypeDefinitions": "goto",
    "search.useIgnoreFiles": false,
    "editor.wordSeparators": "`~!@#%^&*()=+[{]}\\|;:'\",.<>/?",
    "editor.quickSuggestions": {
        "strings": true
    },
    "editor.linkedEditing": true,
    "editor.formatOnSave": false,
    "notebook.output.wordWrap": true,
    "editor.minimap.maxColumn": 250,
    "explorer.confirmDelete": false,
    "editor.fontWeight": "400",
    "editor.inlineSuggest.suppressSuggestions": true,
    "editor.padding.top": 16,
    "workbench.editor.empty.hint": "hidden",
    "workbench.editor.showTabs": "multiple",
    "keyboard.dispatch": "keyCode",
    "workbench.layoutControl.enabled": false,
    "issueReporter.experimental.auxWindow": false,
    "window.commandCenter": false,
    "workbench.preferredHighContrastColorTheme": "Monokai High Contrast",
    "workbench.fontAliasing": "none",
    "workbench.iconTheme": null,
    "breadcrumbs.enabled": false,
    "breadcrumbs.icons": false,
    "breadcrumbs.symbolPath": "off",
    "terminal.integrated.fontFamily": "Space Mono",
    "editor.fontFamily": "Space Mono",
    "editor.suggest.showIcons": false,
    "outline.icons": false,
    "accessibility.dimUnfocused.opacity": 1,
    "editor.foldingHighlight": false,
    "explorer.compactFolders": false,
    "explorer.expandSingleFolderWorkspaces": false,
    "workbench.tree.enableStickyScroll": false,
    "editor.stickyScroll.scrollWithEditor": false,
    "workbench.editor.enablePreview": false,
    "workbench.tree.stickyScrollMaxItemCount": 1,
    "editor.stickyScroll.enabled": false,
    "editor.stickyScroll.maxLineCount": 1,
    "notebook.stickyScroll.mode": "flat",
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": ["u"],
            "after": [],
            "commands": [
                {
                    "command": "undo",
                    "args": []
                }
            ]
        },
        {
            "before": ["<C-r>"],
            "after": [],
            "commands": [
                {
                    "command": "redo",
                    "args": []
                }
            ]
        },
    ],
    "vim.easymotion": true,
    "vim.overrideCopy": false,
    "vim.statusBarColorControl": true,
    "vim.history": 500,
    "vim.statusBarColors.commandlineinprogress": "#56ff01",
    "vim.statusBarColors.easymotioninputmode": "#56ff01",
    "vim.statusBarColors.easymotionmode": "#56ff01",
    "vim.statusBarColors.insert": "#56ff01",
    "vim.statusBarColors.normal": "#56ff01",
    "vim.statusBarColors.replace": "#56ff01",
    "vim.statusBarColors.searchinprogressmode": "#56ff01",
    "vim.statusBarColors.surroundinputmode": "#56ff01",
    "vim.statusBarColors.visual": "#f701ff",
    "vim.statusBarColors.visualblock": "#f701ff",
    "vim.statusBarColors.visualline": "#f701ff",
    "terminal.integrated.defaultLocation": "editor",
    "vim.handleKeys": {
        "<C-d>": false,
        "<C-n>": false,
        "<C-s>": false,
        "<C-z>": false,
    },

    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": "support.type.property-name",
                "settings": {
                    "foreground": "#7f6203"
                }
            },
            {
                "scope": "variable",
                "settings": {
                    "foreground": "#33FF57"
                }
            },
            {
                "scope": "variable.parameter",
                "settings": {
                    "foreground": "#33FF57"
                }
            },
            {
                "scope": "comment",
                "settings": {
                    "foreground": "#9f9c9f"
                }
            },
            {
                "scope": "comment.block",
                "settings": {
                    "foreground": "#9f9c9f"
                }
            },
            {
                "scope": "keyword.control",
                "settings": {
                    "foreground": "#3333ff"
                }
            },
            {
                "scope": "entity.name.function",
                "settings": {
                    "foreground": "#d51bc5"
                }
            },
            {
                "scope": "meta",
                "settings": {
                    "foreground": "#33FF57"
                }
            },
            {
                "scope": "meta.function-call",
                "settings": {
                    "foreground": "#d51bc5"
                }
            },
            {
                "scope": "string",
                "settings": {
                    "foreground": "#9f9c9f"
                }
            }
        ]
    },
    "vim.vimrc.enable": true,
    "vim.vimrc.path": "~/.vimrc",
    "vim.camelCaseMotion.enable": true,
    "vim.highlightedyank.enable": true,
    "vim.useSystemClipboard": true,
    "vim.mouseSelectionGoesIntoVisualMode": false,
    "application.shellEnvironmentResolutionTimeout": 100,
    "editor.autoIndent": "advanced",
    "editor.multiCursorModifier": "ctrlCmd",
    "extensions.experimental.affinity": {
        "asvetliakov.vscode-neovim": 1
    },
    "vim.enableNeovim": true,
    "vim.neovimPath": "/usr/local/bin/nvim",
    "vim.neovimConfigPath": "~/.config/nvim/init.vim",
    "vim.neovimUseConfigFile": true,
    "nvim-ui.nvimColorNormal": "#ffc600",
    "nvim-ui.nvimColorInsert": "#D32F2F",
    "nvim-ui.nvimColorVisual": "#673AB7",
    "nvim-ui.nvimColorReplace": "#000",

}
